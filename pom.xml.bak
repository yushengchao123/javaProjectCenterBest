        </dependency>
	</dependencies>
	   <profiles>
        <profile>
            <id>dev</id>
            <properties>
                <profileActive>dev</profileActive>
                <buildFinal>cwc</buildFinal>
            </properties>
            <activation>
                <activeByDefault>true</activeByDefault>
            </activation>
        </profile>
         <profile>
            <id>product</id>
            <properties>
                <profileActive>product</profileActive>
                <buildFinal>cwc</buildFinal>
            </properties>
        </profile>
    </profiles>

	<build>
		<finalName>${buildFinal}</finalName>
		<resources>
			<resource>
				<directory>src/main/resources</directory>
				<filtering>false</filtering>
				<excludes>
					<exclude>application*.properties</exclude>
					<exclude>conf/dev/*</exclude>
					<exclude>conf/product/*</exclude>
				</excludes>
			</resource>
			<resource>
				<directory>src/main/resources/conf/${profileActive}</directory>
				<filtering>true</filtering>
				<includes>
					<include>application.properties</include>
					<include>conf/${profileActive}/*</include>
				</includes>
			</resource>
		</resources>
		<plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-compiler-plugin</artifactId>
				<version>3.3</version><!--$NO-MVN-MAN-VER$ -->
				<configuration>
					<source>1.7</source>
					<target>1.7</target>
					<encoding>utf-8</encoding>
				</configuration>
			</plugin>
			
			<plugin>
				<groupId>org.springframework.boot</groupId>
				<artifactId>spring-boot-maven-plugin</artifactId>
				<dependencies>
					<dependency>
					<groupId>org.springframework</groupId>
					<artifactId>springloaded</artifactId>
					<version>1.2.6.RELEASE</version>
					</dependency>
				</dependencies>
			</plugin>

			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
				<artifactId>maven-surefire-plugin</artifactId>
				<configuration>
					<useSystemClassLoader>false</useSystemClassLoader>
				</configuration>
			</plugin>

		</plugins>
	</build>


</project>
